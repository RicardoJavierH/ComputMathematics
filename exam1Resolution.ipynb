    "#First question\n",
    "def matriz(n):\n",
    "    A = np.zeros((n,n))\n",
    "    for i in range(0,n):\n",
    "        for j in range(0,n):\n",
    "            A[i,j] = 1/(i+1+j+1-1)\n",
    "    return A\n",
    "\n",
    "print(matriz(5))"
